<html lang="en">
<head>
    <meta charset="utf-8">
</head>
<body>
<h1>Cinemas</h1>
<ul>
    {% for movie in movies %}
        <li>{{ movie.movie.title }}</li>
        <ul>
            {% for cinema in movie.cinemas %}
                <li>{{ cinema.name }}</li>
                <ul>
                    {% for st in cinema.show_times %}
                        <li>{{ st }}</li>
                    {% endfor %}
                </ul>
            {% endfor %}
        </ul>
    {% endfor %}
</ul>
</body>
</html>
